@model IEnumerable<Message>

@foreach (var message in Model)
{
    <div>
        <h3>@message.MessageTitle</h3>
        <div class="row">
            <div class="col-sm-9">
                <p>@message.MessageContent</p>
            </div>
            <div class="col-sm-3">
                Email Message here<br />
                @message.Created.ToString("dd-MM-yyyy")
            </div>
        </div>
    </div>
}
